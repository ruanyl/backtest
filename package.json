  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruanyl/backtest/issues"
  },
  "homepage": "https://github.com/ruanyl/backtest",
  "dependencies": {
    "ramda": "^0.24.1",
    "with-props": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.4.1",
